{
    "name": "gelbhavanitradin01",
    "version": "1.0.0",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder --win portable --x64"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "electron": "^39.1.2",
        "electron-builder": "^24.6.0"
    },
    "build": {
        "appId": "com.yourcompany.yourapp",
        "productName": "MyApp",
        "files": [
            "**/*"
        ],
        "win": {
            "target": [
                "portable"
            ]
        },
        "nsis": {
            "perMachine": false
        },
        "portable": {
            "artifactName": "${productName}-portable-${version}-${arch}.${ext}"
        }
    }
}